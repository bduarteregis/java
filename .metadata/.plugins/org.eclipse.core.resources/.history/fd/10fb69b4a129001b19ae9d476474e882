
public class TestaPontoFlutuante {
	
	public static void main(String[] args) {
		
		double salario;
		salario = 1000.50;
		System.out.println("O salário mínimo é " + salario);
		
		double divisao = 10 / 5;
		System.out.println(divisao);
		
		int outraDivisao = 5 / 2;
		System.out.println(outraDivisao);
	}

}
